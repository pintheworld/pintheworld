/*
* added dev-build and eslint in scripts to run the project in command prompt using these scripts
* modified name and url of this project
* added bootstrap, angular google map, eslint and other dependencies.
*/
{
  "name": "PintheWorld",
  "version": "0.0.1",
  "scripts": {
    "start": "webpack-dev-server --inline --progress --port 8080",
    "test": "karma start",
    "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "dev-build": "rimraf ../python-src/webapp && webpack --config config/webpack.dev.js --progress --watch",
    "eslint": "./node_modules/.bin/eslint src/app/ || true"
